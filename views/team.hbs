{{> header}}
<div class="page-body">
    <div class="page-cover-photo team-photo">
        <div class="page-heading">Meet the Team</div>
    </div>
    <div class="page-wrapper">
        <div class="page-content">
            <div class="card-title">2017-2018 Board Members</div>
            <section class="section">
                <div class="section-container">
                    <div class="container team-container column">
                        <div class="container team-container constrained">
                            <div class="grid align-start">
                                <div id="officers-mount"></div>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
            
        </div>
    </div>
</div>
<!--  OFFICERS HANDLEBARS SCRIPT -->
<script id="officers-template" type="text/x-handlebars-template">
    \{{#each officers}}
    <div class="col sm-12 md-8 lg-8 person-wrapper">
        <div class="person-card">
            <img class="profile-picture" src="\{{this.imgUrl}}">
            <h4>\{{this.name}}</h4>
            <h5>\{{this.role}}</h5>
            <a href="\{{this.email}}"><i id="social-em" class="fa fa-envelope fa-2x social"></i></a>
            <p>\{{this.bio}}</p>
        </div>
    </div>
    \{{/each}}
</script>
{{> footer}}
